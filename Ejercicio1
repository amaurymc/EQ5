#include <stdio.h>
#include <math.h>

//Dado n, calcular suma de los factores primos.

//
int primo = 1;
int cont = 0;
int n = 9;

float cck;
int sum;

int factores[100];

int main(void) 
{
	for (cont = 0; primo < n; primo++)
	{
	 cck = n/primo;
	 if(cck==floor(cck))
	 {
      factores[cont];
	 }
	 cont++;
	 //printf("%s\n", cont);
	}
	
	return factores[0];
}
